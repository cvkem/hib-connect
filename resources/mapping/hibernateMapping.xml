<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="vinzi.cdfdeMgt.domain">

  <class name="TrackInfo" table="track_info">
    <id name="id" column="TRACK_ID">
      <generator class="native"/>
    </id>
    <property name="track_name"/>
    <property name="file_location"/>
  </class>


</hibernate-mapping>

